{
  "name": "node-red-contrib-ai-sombrero",
  "version": "0.2.9",
  "description": "AI Workflow Orchestration for Node-RED",
  "main": "index.js",
  "scripts": {
    "start": "node-red -s settings.js -u .node-red",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "node-red": {
    "version": ">=3.0.0",
    "nodes": {
      "llm-config": "nodes/llm-config/llm-config.js",
      "llm-connector": "nodes/llm-connector/llm-connector.js",
      "mcp-node": "nodes/mcp-node/mcp-node.js",
      "dbconfig-node": "nodes/dbconfig-node/dbconfig.js",
      "role-manager": "nodes/shared/role-manager/index.js"
    },
    "resources": {
      "shared/role-manager/ui.js": "nodes/shared/role-manager/ui.js",
      "shared/ui/prompt-enhancer-ui.js": "shared/ui/prompt-enhancer-ui.js"
    }
  },
  "keywords": [
    "node-red",
    "ai",
    "workflow",
    "automation",
    "llm",
    "language-model",
    "orchestration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "anthropic": "0.0.0",
    "axios": "^1.6.5",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "mysql2": "^3.9.1",
    "node-red": "^3.0.0",
    "openai": "^4.24.1",
    "pg": "^8.11.3",
    "sqlite": "^5.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@types/jest": "^29.5.0",
    "app-module-path": "^2.2.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
