{
  "name": "node-red-contrib-dbconfig",
  "version": "0.0.1",
  "description": "Node-RED configuration node for database connections (PostgreSQL, MySQL, SQLite).",
  "main": "dbconfig.js",
  "node-red": {
    "nodes": {
      "dbconfig": "dbconfig.js"
    }
  },
  "keywords": [
    "node-red",
    "database",
    "config",
    "postgresql",
    "mysql",
    "sqlite"
  ],
  "author": "Windsurf AI",
  "license": "MIT",
  "dependencies": {
    "pg": "8.11.3",
    "mysql2": "3.6.5",
    "sqlite": "4.2.0",
    "sqlite3": "5.1.7"
  }
}
